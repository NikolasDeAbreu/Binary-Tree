class NodoArvore:
    def __init__(self, chave=None, esquerda=None, direita=None):
        self.key = chave
        self.left = esquerda
        self.right = direita

    def __repr__(self):
        return '%s <- %s -> %s' % (self.left and self.left.key,
                                    self.key,
                                    self.right and self.right.key)

    def inserir(self,valor,aux=None):
        if self.key is None:
            self.key = valor
        else:
            if valor < self.key:
                if self.left is None:
                    NovoNodo = NodoArvore(valor)
                    self.left = NovoNodo
                else:
                    aux = self.left
                    aux.inserir(valor)
            else:
                if self.right is None:
                    NovoNodo = NodoArvore(valor)
                    self.right = NovoNodo
                else:
                    aux = self.right
                    aux.inserir(valor)

    def preOrder(self):
        if self.left is not None:
            self.left.preOrder()
            if self.right is not None:
                print(self.key)
                self.right.preOrder()
        else:
            if self.right is not None:
                print(self.key)
                self.right.preOrder()
            else:
                print(self.key)

val = [4,1,2,0,6,5,7]
a = NodoArvore()
for itr in val:
    a.inserir(itr)
a.preOrder()
